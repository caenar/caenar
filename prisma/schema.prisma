generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model projects {
  id         BigInt   @id @default(autoincrement())
  title      String
  desc       String?
  created_at DateTime @default(now()) @db.Timestamptz(6)
}
